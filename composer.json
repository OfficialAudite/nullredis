{
  "name": "officialaudite/nullredis",
  "description": "A tiny file-backed Redis-like shim for PHP (KV, sets, sorted sets)",
  "type": "library",
  "license": "MIT",
  "keywords": ["redis", "cache", "rate limit", "file cache", "shim"],
  "authors": [
    { "name": "Audite.dev" }
  ],
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "scripts": {
    "test": "phpunit --configuration phpunit.xml.dist"
  },
  "autoload": {
    "psr-4": {
      "NullRedis\\": "src/"
    }
  },
  "support": {
    "issues": "https://github.com/officialaudite/nullredis/issues",
    "source": "https://github.com/officialaudite/nullredis"
  }
}

